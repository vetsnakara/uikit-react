{
    "name": "@vetsnakara/react-uikit",
    "version": "1.0.13",
    "description": "React based UIKit with PRR styles",
    "main": "lib/react-uikit.js",
    "types": "lib/types/index.d.ts",
    "scripts": {
        "prod": "cross-env PRRENV=production gulp production",
        "prod2": "grunt prod",
        "dev": "grunt rdev",
        "rdev": "grunt rdev",
        "build:prod": "cross-env NODE_ENV=production webpack",
        "build:dev": "cross-env NODE_ENV=development webpack",
        "build:assets": "npm run prod2",
        "build:all": "npm-run-all clear build:assets -p build:prod build:dev gen-types -s move-bundle",
        "clear": "rimraf ./lib ./assets",
        "gen-types": "tsc -p ./tsconfig.build.json",
        "check-assets-dir": "node ./scripts/checkAssetsDir",
        "move-bundle": "node ./scripts/moveBundleToAssets",
        "styletest": "node autotest.js",
        "storybook:dev": "storybook dev -p 3000",
        "storybook:build": "storybook build",
        "storybook": "npm-run-all check-assets-dir storybook:dev",
        "prepare": "husky install",
        "format": "prettier ./src/**/*.{js,ts,tsx} --write",
        "lint": "eslint ./src --ext .js,.ts,jsx",
        "lint:fix": "npm run lint -- --fix "
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vetsnakara/react-uikit.git"
    },
    "keywords": [
        "react",
        "uikit"
    ],
    "author": "BFT",
    "license": "MIT",
    "homepage": "https://github.com/vetsnakara/react-uikit#readme",
    "devDependencies": {
        "@babel/core": "7.23.6",
        "@babel/plugin-proposal-class-properties": "7.18.6",
        "@babel/preset-env": "7.23.6",
        "@babel/preset-react": "7.23.3",
        "@storybook/addon-docs": "^7.6.10",
        "@storybook/addon-essentials": "^7.6.10",
        "@storybook/blocks": "7.6.6",
        "@storybook/react": "7.6.6",
        "@storybook/react-webpack5": "7.6.6",
        "@storybook/test": "7.6.6",
        "@typescript-eslint/eslint-plugin": "^6.18.0",
        "@typescript-eslint/parser": "^6.18.0",
        "autoprefixer": "9.8.6",
        "babel-loader": "9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.8.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.2",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "grunt": "1.5.3",
        "grunt-babel": "8.0.0",
        "grunt-contrib-concat": "2.1.0",
        "grunt-contrib-connect": "3.0.0",
        "grunt-contrib-copy": "1.0.0",
        "grunt-contrib-cssmin": "4.0.0",
        "grunt-contrib-uglify": "5.2.2",
        "grunt-contrib-watch": "1.1.0",
        "grunt-exec": "3.0.0",
        "grunt-postcss": "0.9.0",
        "grunt-sass": "3.1.0",
        "grunt-stylelint": "0.18.0",
        "grunt-svgmin": "6.0.1",
        "grunt-svgstore": "2.0.0",
        "gulp": "4.0.2",
        "gulp-autoprefixer": "8.0.0",
        "gulp-babel": "8.0.0",
        "gulp-clean-css": "4.3.0",
        "gulp-concat": "2.6.1",
        "gulp-connect": "5.7.0",
        "gulp-dart-sass": "1.0.2",
        "gulp-if": "3.0.0",
        "gulp-rename": "2.0.0",
        "gulp-sourcemaps": "3.0.0",
        "gulp-svgmin": "4.0.0",
        "gulp-svgo": "2.2.1",
        "gulp-svgstore": "9.0.0",
        "gulp-terser": "2.1.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "msw-storybook-addon": "^1.10.0",
        "npm-run-all": "^4.1.5",
        "open": "7.4.2",
        "postcss": "8.4.16",
        "postcss-scss": "4.0.4",
        "prettier": "^3.1.1",
        "rimraf": "^5.0.5",
        "sass": "1.53.0",
        "sass-embedded": "1.53.0",
        "storybook": "7.6.6",
        "style-loader": "^3.3.3",
        "stylelint": "14.12.0",
        "stylelint-config-standard-scss": "5.0.0",
        "stylelint-order": "5.0.0",
        "time-grunt": "2.0.0",
        "typescript": "5.2.2",
        "webpack": "5.89.0",
        "webpack-cli": "5.1.4"
    },
    "dependencies": {
        "@hookform/resolvers": "3.3.4",
        "@tanstack/react-query": "4.36.1",
        "@types/react": "18.2.45",
        "axios": "1.6.7",
        "classnames": "2.3.2",
        "inputmask": "5.0.8",
        "prop-types": "15.8.1",
        "react-hook-form": "7.49.2"
    },
    "peerDependencies": {
        "axios": "1.6.7",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.49.2",
        "@tanstack/react-query": "4.36.1"
    },
    "browserslist": [
        "IE 11",
        "Firefox >= 71",
        "Chrome >= 78",
        "Edge >= 76",
        "Safari >= 13",
        "Opera >= 64",
        "> 2%"
    ],
    "lint-staged": {
        "*.(css|scss)": [
            "npm run styletest"
        ],
        "./src/**/*.(js|ts|jsx)": [
            "npx prettier",
            "npx eslint --fix"
        ]
    }
}
