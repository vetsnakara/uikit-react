{
    "name": "@vetsnakara/uikit-react",
    "version": "0.0.20",
    "description": "UIKit React with PRR styles",
    "main": "lib/uikit-react.js",
    "types": "lib/types/index.d.ts",
    "scripts": {
        "prod": "cross-env PRRENV=production gulp production",
        "prod2": "grunt prod",
        "dev": "grunt rdev",
        "rdev": "grunt rdev",
        "build:all": "npm-run-all clear build:assets --parallel build:prod build:dev gen-types -s move-bundle",
        "build:prod": "cross-env NODE_ENV=production webpack",
        "build:dev": "cross-env NODE_ENV=development webpack",
        "build:assets": "npm run prod",
        "sb": "run-s check-assets-dir sb:dev",
        "sb:dev": "storybook dev -p 3000",
        "sb:build": "rm -rf ./storybook-static && storybook build",
        "sb:serve": "http-server ./storybook-static -o --port 3000 --silent",
        "sb:build-serve": "run-s sb:build sb:serve",
        "wait:sb": "wait-on tcp:3000",
        "format": "prettier ./src/**/*.{js,ts,tsx} --write",
        "lint": "eslint ./src --ext .js,.ts,jsx",
        "lint:fix": "npm run lint -- --fix ",
        "vreg": "npm run clear:vreg; npm run vreg:test; npm run vreg:report; npm run vreg:check",
        "vreg:test": "concurrently --kill-others --success last --names \"SB,LOKI:TEST\" --prefix-colors \"magenta,blue\" \"npm run sb:build-serve\" \"npm run loki:test:wait-sb\"",
        "vreg:upd": "concurrently --kill-others --success last --names \"SB,LOKI:UPD\" --prefix-colors \"magenta,blue\" \"npm run sb:build-serve\" \"npm run loki:upd:wait-sb\"",
        "vreg:check": "node scripts/checkVRegResult.js",
        "vreg:inspect": "http-server ./.loki",
        "vreg:ok": "loki approve",
        "loki:upd": "loki update --port 3000",
        "loki:upd:wait-sb": "run-s wait:sb loki:upd",
        "loki:test": "loki test --port 3000",
        "loki:test:wait-sb": "run-s wait:sb loki:test",
        "loki:approve": "loki approve",
        "vreg:report": "run-s vreg:report:json vreg:report:html",
        "vreg:report:json": "node scripts/generateVisualJsonReport.js",
        "vreg:report:html": "reg-cli --from .loki/report.json  --report .loki/index.html",
        "gen-types": "tsc -p ./tsconfig.build.json",
        "check-assets-dir": "node ./scripts/checkAssetsDir",
        "move-bundle": "node ./scripts/moveBundleToAssets",
        "styletest": "node autotest.js",
        "clear": "rm -rf ./lib ./assets",
        "clear:vreg": "rm -rf ./.loki/current ./.loki/difference ./.loki/report.*",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vetsnakara/uikit-react.git"
    },
    "keywords": [
        "react",
        "uikit"
    ],
    "author": "BFT",
    "license": "MIT",
    "homepage": "https://github.com/vetsnakara/uikit-react#readme",
    "devDependencies": {
        "@babel/core": "7.23.6",
        "@babel/plugin-proposal-class-properties": "7.18.6",
        "@babel/preset-env": "7.23.6",
        "@babel/preset-react": "7.23.3",
        "@storybook/addon-docs": "^7.6.10",
        "@storybook/addon-essentials": "^7.6.10",
        "@storybook/blocks": "7.6.6",
        "@storybook/react": "7.6.6",
        "@storybook/react-webpack5": "7.6.6",
        "@storybook/test": "7.6.6",
        "@typescript-eslint/eslint-plugin": "^6.18.0",
        "@typescript-eslint/parser": "^6.18.0",
        "autoprefixer": "9.8.6",
        "babel-loader": "9.1.3",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.2",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "grunt": "1.5.3",
        "grunt-babel": "8.0.0",
        "grunt-contrib-concat": "2.1.0",
        "grunt-contrib-connect": "3.0.0",
        "grunt-contrib-copy": "1.0.0",
        "grunt-contrib-cssmin": "4.0.0",
        "grunt-contrib-uglify": "5.2.2",
        "grunt-contrib-watch": "1.1.0",
        "grunt-exec": "3.0.0",
        "grunt-postcss": "0.9.0",
        "grunt-sass": "3.1.0",
        "grunt-stylelint": "0.18.0",
        "grunt-svgmin": "6.0.1",
        "grunt-svgstore": "2.0.0",
        "gulp": "4.0.2",
        "gulp-autoprefixer": "8.0.0",
        "gulp-babel": "8.0.0",
        "gulp-clean-css": "4.3.0",
        "gulp-concat": "2.6.1",
        "gulp-connect": "5.7.0",
        "gulp-dart-sass": "1.0.2",
        "gulp-if": "3.0.0",
        "gulp-rename": "2.0.0",
        "gulp-sourcemaps": "3.0.0",
        "gulp-svgmin": "4.0.0",
        "gulp-svgo": "2.2.1",
        "gulp-svgstore": "9.0.0",
        "gulp-terser": "2.1.0",
        "http-server": "^14.1.1",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "loki": "^0.34.0",
        "msw-storybook-addon": "^1.10.0",
        "npm-run-all": "^4.1.5",
        "open": "7.4.2",
        "postcss": "8.4.16",
        "postcss-scss": "4.0.4",
        "prettier": "^3.1.1",
        "reg-cli": "^0.18.0",
        "sass": "1.53.0",
        "sass-embedded": "1.53.0",
        "storybook": "7.6.6",
        "storycap": "^4.2.0",
        "stylelint": "14.12.0",
        "stylelint-config-standard-scss": "5.0.0",
        "stylelint-order": "5.0.0",
        "time-grunt": "2.0.0",
        "typescript": "5.2.2",
        "wait-on": "^7.2.0",
        "webpack": "5.89.0",
        "webpack-cli": "5.1.4"
    },
    "dependencies": {
        "@hookform/resolvers": "3.3.4",
        "@tanstack/react-query": "4.36.1",
        "@types/react": "18.2.45",
        "axios": "1.6.7",
        "classnames": "2.3.2",
        "inputmask": "5.0.8",
        "prop-types": "15.8.1",
        "react-hook-form": "7.49.2"
    },
    "peerDependencies": {
        "@tanstack/react-query": "4.36.1",
        "axios": "1.6.7",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.49.2"
    },
    "browserslist": [
        "IE 11",
        "Firefox >= 71",
        "Chrome >= 78",
        "Edge >= 76",
        "Safari >= 13",
        "Opera >= 64",
        "> 2%"
    ],
    "lint-staged": {
        "*.(css|scss)": [
            "npm run styletest"
        ],
        "./src/**/*.(js|ts|jsx)": [
            "npx prettier",
            "npx eslint --fix"
        ]
    },
    "loki": {
        "configurations": {
            "chrome.laptop": {
                "target": "chrome.docker",
                "width": 1366,
                "height": 768,
                "deviceScaleFactor": 1,
                "mobile": false
            }
        }
    }
}
